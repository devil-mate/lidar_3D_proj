<?xml version="1.0"?>
<launch>
  
  <arg name="prefix" default="summit_xl_"/>
  <arg name="rviz_config" default="$(find summit_xl_gazebo)/rviz/summitxl_a_std.rviz"/>
  <arg name="robot_model"  default="summit_xl_std.urdf.xacro"  doc="Different robot versions: ('')"/>
  
	<arg name="gps_latitude" default="39.5080331"/>
	<arg name="gps_longitude" default="-0.4619816"/>
  <arg name="xacro_robot" default="summit_xl_std.urdf.xacro"/>
  <arg name="ros_planar_move_plugin" default="false"/>
  <arg name="omni_drive" default="false"/>
   <arg name="use_gpu_for_simulation" default="false"/><!-- use_gpu_for_simulation -->
  <!-- Load the URDF into ROS parameter server -->
  <!-- <arg name="urdf_file" default="$(find xacro)/xacro '$(find summit_xl_description)/robots/$(arg robot_model)' prefix:=$(arg prefix) " /> -->
  <!-- <param name="robot_description" command="$(arg urdf_file)" /> -->
  <arg name="urdf_file" default="$(find xacro)/xacro '$(find summit_xl_description)/robots/$(arg xacro_robot)'
          prefix:=$(arg prefix)
          ros_planar_move_plugin:=$(arg ros_planar_move_plugin)
          omni_wheels:=$(arg omni_drive)
          gpu:=$(arg use_gpu_for_simulation)
          gps_latitude:='$(arg gps_latitude)'
          gps_longitude:='$(arg gps_longitude)'
          --inorder"/>
  <param name="robot_description" command="$(arg urdf_file)" /> 
  <!-- <param name="robot_description" command="$(find xacro)/xacro '$(find summit_xl_description)/robots/$(arg xacro_robot)'
          prefix:=$(arg prefix)
          ros_planar_move_plugin:=$(arg ros_planar_move_plugin)
          omni_wheels:=$(arg omni_drive)
          gpu:=$(arg use_gpu_for_simulation)
          gps_latitude:='$(arg gps_latitude)'
          gps_longitude:='$(arg gps_longitude)'
          inorder"/> -->


  
</launch>

